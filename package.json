{
  "name": "@kremen/api",
  "version": "1.1.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:eslint": "eslint \"{core,services,utils}/**\"",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "format": "prettier --write \"{core,services,utils}/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@kremen/core": "1.0.11",
    "cheerio": "1.0.0-rc.3",
    "iconv-lite": "0.5.1",
    "redis": "3.0.2",
    "request": "2.88.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.44",
    "@types/cheerio": "0.22.16",
    "@types/node": "13.7.4",
    "@types/redis": "2.8.16",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "eslint": "6.8.0",
    "prettier": "1.19.1",
    "serverless-webpack": "^5.1.1",
    "source-map-support": "^0.5.6",
    "ts-loader": "^4.2.0",
    "typescript": "3.8.2",
    "webpack": "^4.5.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",

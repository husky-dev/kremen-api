{
  "name": "kremen-cinemas-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn start:prd",
    "start:prd": "micro -l tcp://0.0.0.0:${PORT-8080} dist/index.js",
    "start:dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"yarn dist && yarn micro-dev -p 8080 -s dist/index.js\"",
    "dist": "tsc && tsc-alias",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint \"src/**\""
  },
  "dependencies": {
    "axios": "0.25.0",
    "micro": "9.3.4"
  },
  "devDependencies": {
    "@types/micro": "7.3.6",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "8.8.0",
    "micro-dev": "3.0.0",
    "nodemon": "2.0.15",
    "tsc-alias": "1.5.0",
    "typescript": "4.5.5"
  }
}

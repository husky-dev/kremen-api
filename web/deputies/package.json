{
  "name": "@kremen/web-deputies",
  "version": "2.1.0",
  "title": "Карта виборчих округів м. Кременчук",
  "company": "#Kremen.Dev",
  "description": "Карта виборчих округів дозволяє вам дізнатись хто є депутатом вашого району та як з ним зв'язатись",
  "url": "https://deputat.kremen.dev/",
  "main": "src/index.tsx",
  "author": "Jaroslav Khorishchenko <websnipter@gmail.com>",
  "license": "ISC",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "webpack-dev-server --mode development --env.ENVFILE=.env.dev",
    "start:loc": "webpack-dev-server --mode development --env.ENVFILE=.env.loc",
    "dist": "yarn dist:dev",
    "dist:dev": "rm -rf dist/* && webpack -p --env.ENVFILE=.env.dev",
    "dist:prd": "rm -rf dist/* && webpack -p --env.ENVFILE=.env.prd",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint \"src/**\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "data:update": "./scripts/init_data_update.sh"
  },
  "dependencies": {
    "@material-ui/core": "4.4.0",
    "@material-ui/icons": "4.4.1",
    "@material-ui/styles": "4.3.3",
    "color": "3.1.3",
    "lodash": "4.17.20",
    "mixpanel-browser": "2.39.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-google-maps": "9.4.5",
    "react-places-autocomplete": "7.2.1",
    "react-router-dom": "5.0.1"
  },
  "devDependencies": {
    "@types/color": "3.0.1",
    "@types/googlemaps": "3.37.6",
    "@types/lodash": "4.14.162",
    "@types/mixpanel-browser": "2.35.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-places-autocomplete": "7.2.5",
    "@types/react-router-dom": "4.3.5",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.0.4",
    "css-loader": "3.2.0",
    "dotenv": "8.1.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.12.0",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "ts-loader": "6.0.4",
    "tslint": "5.19.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.6.2",
    "typescript-tslint-plugin": "0.5.4",
    "url-loader": "2.1.0",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.7",
    "webpack-dev-server": "3.8.0"
  }
}

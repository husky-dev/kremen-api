{
  "name": "@kremen/transport-web",
  "version": "0.0.17",
  "title": "Транспорт",
  "description": "Карта кременчуцького транспорту",
  "company": "Кремінь",
  "url": "https://transport.kremen.dev",
  "main": "src/index.tsx",
  "author": "Jaroslav Khorishchenko <websnipter@gmail.com>",
  "license": "ISC",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "webpack-dev-server --mode development --env.ENVFILE=.env.dev",
    "start:loc": "webpack-dev-server --mode development --env.ENVFILE=.env.loc",
    "dist": "yarn dist:dev",
    "dist:dev": "rm -rf dist/* && webpack -p --env.ENVFILE=.env.dev",
    "dist:prd": "rm -rf dist/* && webpack -p --env.ENVFILE=.env.prd",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint \"src/**\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "storybook": "start-storybook -p 6006"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*.ts,*tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "axios": "0.19.2",
    "color": "3.1.3",
    "lodash": "4.17.20",
    "mixpanel-browser": "2.34.0",
    "moment": "2.29.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-google-maps": "9.4.5",
    "react-markdown": "4.3.1",
    "react-motion": "0.5.2",
    "react-redux": "7.2.1",
    "react-responsive": "8.0.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-links": "5.3.14",
    "@storybook/addons": "5.3.14",
    "@storybook/react": "5.3.14",
    "@types/color": "3.0.1",
    "@types/googlemaps": "3.39.3",
    "@types/lodash": "4.14.162",
    "@types/mixpanel-browser": "2.33.1",
    "@types/moment": "2.13.0",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-motion": "0.0.29",
    "@types/react-redux": "7.1.9",
    "@types/react-responsive": "8.0.2",
    "@types/react-router-dom": "5.1.3",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.18.3",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "raw-loader": "4.0.0",
    "redux-devtools-extension": "2.13.8",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "ts-loader": "6.2.1",
    "typescript": "3.8.2",
    "url-loader": "3.0.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  }
}
